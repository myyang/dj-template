[run]
omit = {{cookiecutter.project_name}}/{{cookiecutter.project_name}}/settings/*
include = {{cookiecutter.project_name}}/*
[report]
exclude_lines=
    pragma: no cover
    def __repr__
    def __str__
    if settings.DEBUG:
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
